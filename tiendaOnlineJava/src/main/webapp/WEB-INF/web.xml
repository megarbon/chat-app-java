<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>ProcessLoginServlet</servlet-name>
        <servlet-class>mgb.controller.ProcessLoginServlet</servlet-class>
        <url-pattern>/ProcessLoginServlet</url-pattern>
    </servlet>
    <servlet>
        <servlet-name>ProcessRegisterServlet</servlet-name>
        <servlet-class>mgb.controller.ProcessRegisterServlet</servlet-class>
        <url-pattern>/ProcessRegisterServlet</url-pattern>
    </servlet>
    <servlet>
        <servlet-name>ProcessPurchaseServlet</servlet-name>
        <servlet-class>mgb.controller.ProcessPurchaseServlet</servlet-class>
        <url-pattern>/ProcessPurchaseServlet</url-pattern>
    </servlet>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
